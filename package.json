{
  "name": "artifacts-size-based-cleanup-action",
  "description": "Cleanup artifacts base on the size limit to make sure the storage space is not exhausted",
  "version": "1.3.0",
  "type": "module",
  "main": "src/index.ts",
  "packageManager": "yarn@4.12.0",
  "scripts": {
    "start": "ncc build --target es2022 && node dist/index.js",
    "build": "ncc build --target es2022",
    "watch": "ncc build -w --target es2022",
    "postinstall": "husky"
  },
  "keywords": [
    "github",
    "remove",
    "delete",
    "artifact"
  ],
  "author": "Ruofan",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/zhuxb711/artifacts-size-based-cleanup-action"
  },
  "dependencies": {
    "@actions/artifact": "^6.1.0",
    "@actions/core": "^3.0.0",
    "@actions/github": "^9.0.0",
    "@octokit/core": "^7.0.6",
    "@octokit/plugin-retry": "^8.0.3",
    "@octokit/plugin-throttling": "^11.0.3",
    "archiver": "^7.0.1",
    "bytes": "^3.1.2",
    "glob": "^13.0.1",
    "lodash": "^4.17.23",
    "pretty-error": "^4.0.0",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@types/archiver": "^7.0.0",
    "@types/bytes": "^3.1.5",
    "@types/glob": "^9.0.0",
    "@types/lodash": "^4.17.23",
    "@types/node": "^25.2.2",
    "@types/uuid": "^11.0.0",
    "husky": "^9.1.7",
    "typescript": "^5.9.3"
  }
}
