    "archiver": "^7.0.1",
    "bytes": "^3.1.2",
    "glob": "^11.0.3",
    "lodash": "^4.17.21",
    "pretty-error": "^4.0.0",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@types/archiver": "^6.0.3",
    "@types/bytes": "^3.1.5",
    "@types/glob": "^9.0.0",
    "@types/lodash": "^4.17.20",
